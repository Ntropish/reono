{
  "extends": "@reono/typescript-config/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "@reono/server",
    "paths": {
      "@/*": ["./*"]
    },
    "moduleResolution": "Bundler"
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules", "**/*.server.tsx"]
}
